/**app.wxss**/
.oneline{
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.twoline{
  text-overflow: ellipsis;
  word-break: break-all;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp:2;
  line-clamp:2;
  -webkit-box-orient: vertical;
}
.flexwrap{
  display: flex;
  display: -webkit-flex;
}
.activeBtn{
  background-color: rgba(255, 255, 255, 0.6)
}